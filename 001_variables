# consulta el tipo de dato
print('Soy un dato tipo string')  # str
print(4)  # int
print(4.4)  # float
print(True)  # Bool
print(3+1j)  # complex


print(type('Soy un dato tipo'))
print(type(4))
print(type(4.3))
print(type(True))
print(type(3+1j))

# usa de variables

my_variable = 'My String Variable'  # se le le asigna un valor a la variable

print(my_variable)  # se imprime la variabless
